@{
    ViewBag.Title = "Контакты";
}

<h2>Контакты</h2>


<table class="table-responsive">

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Svyazy)
            </td>
        </tr>
    }

</table>
